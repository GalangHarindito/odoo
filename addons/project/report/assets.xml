<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="my_assets">
        <t t-call="web.html_container">
        <t t-foreach="docs" t-as="doc">
            <h2>Print My Assets</h2>
            Assets No: <t t-esc="doc.assetsNo" />
            Owner: <t t-esc="doc.owner" />
            Description: <t t-esc="doc.description" />
            Location: <t t-esc="doc.location" />
            Date: <t t-esc="doc.date" />
        </t>
    </t>
    </template>
</odoo>